class Persona:
    def init(nombre, edad):
        self.nombre = nombre
        self.edad = edad
    
    def saludar:
        return "Hola saludar"


class Gerente(Persona):
	def init(nombre, edad, departamento):
		super.init(nombre, edad)
		self.departamento = departamento
	
	def saludar:
		return "Soy el gerente del departamento"

let jorge = Gerente("Jorge", 50, "Ventas")
print(jorge.saludar())