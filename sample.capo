# Crear una clase
class Cocoyer:
	def init(nombre, ciudad):
		self.nombre = nombre
		self.ciudad = ciudad
	
	def saludar:
		println(f"Hola me llamo $self.nombre y vivo en $self.ciudad")


# Crear una instancia
let gerardo = Cocoyer("Gerardito", "Madeira")
gerardo.saludar()

